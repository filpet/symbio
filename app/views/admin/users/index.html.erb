<div class="span-22 prepend-1 append-1 last">

	<h2>Benutzer</h2>
	<table>
		<tr>
			<th>
				ID
			</th>
			<th>
				Name
			</th>
			<th>
				E-Mail
			</th>
		</tr>
		<% @users.each do |user| %>
		<tr>
			<td>
				<%=h user.id %>
			</td>
			<td>
				<%=h user.name %>
			</td>
			<td>
				<%=h user.email %>
			</td>
		</tr>
		<% end %>
	</table>

</div>
